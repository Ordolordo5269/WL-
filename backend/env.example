# Variables de entorno para el backend
# Copia este archivo como .env y reemplaza los valores

# Token de Mapbox (obtener en https://www.mapbox.com/)
MAPBOX_TOKEN=tu_token_de_mapbox_aqui

# Puerto del servidor backend
PORT=3001

# Entorno de desarrollo
NODE_ENV=development

# Configuraci√≥n de CORS (opcional)
CORS_ORIGIN=http://localhost:5173 

# Base de datos (PostgreSQL + Prisma)
DATABASE_URL=postgresql://worldlore:worldlore@localhost:5432/worldlore?schema=public

# JWT Secret (generate a strong random string for production)
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# DeepSeek API Key (for predictive analysis insights)
DEEPSEEK_API_KEY=your-deepseek-api-key-here