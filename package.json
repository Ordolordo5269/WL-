{
  "name": "worldlore",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n BACKEND,FRONTEND -c green,cyan \"npm:backend:dev\" \"npm:frontend:dev\"",
    "backend:dev": "npm --prefix backend run dev",
    "backend:build": "npm --prefix backend run build",
    "backend:start": "npm --prefix backend run start",
    "frontend:dev": "npm --prefix frontend run dev",
    "frontend:build": "npm --prefix frontend run build",
    "frontend:preview": "npm --prefix frontend run preview"
  },
  "dependencies": {
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "react-chrono": "^2.9.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/morgan": "^1.9.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "concurrently": "^9.1.0"
  }
}
